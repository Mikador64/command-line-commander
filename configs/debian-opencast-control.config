[
  'Debian Opencast Command & Control',
  {
    'cmd' => 'tail -f -n 25 /var/log/opencast/opencast.log',
    'title' => 'Tail Opencast Logs [STOP]',
    'stop' => 1
  },
  {
    'cmd' => 'sudo journalctl -u opencast.service',
